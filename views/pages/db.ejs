<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>



<div class="container">
<h2>Database Results</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
            <th><a name="" id="" class="btn btn-secondary" href="#" role="button">Add New</a></th>
        </tr>
    </thead>
    <tbody>
    <% results.forEach(function(r) { %>
        <tr>
            <td><%= r.id %></td>
            <td><%= r.title %></td>
            <td><%= r.price %></td>
        </tr>
    <% }); %>    
    </tbody>
</table>


</div>
<% include ../partials/footer.ejs %>
</body>
</html>