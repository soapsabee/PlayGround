<div class="container">
    <br>
    <br>
    <form action="/users_list/save" method="post">
   <div class="form-group" style="">
     <label for="ID">ID</label>
     <input type="text" class="form-control" style="width:500px;" name="id" id="" aria-describedby="helpId" value="<%= results[0].id %>" readonly="true">
     <label for="ID">Email</label>
     <input type="text" class="form-control" style="width:500px;" name="email" id="" aria-describedby="helpId" value="<%= results[0].email %>">
     <label for="ID">Password</label>
     <input type="text" class="form-control" style="width:500px;" name="password" id="" aria-describedby="helpId" value="<%= results[0].password %>">
     <label for="date">Create At</label>
     <br>
     
    
     <input id="datepicker" name="create_at" width="276" value="<%= results[0].create_at %>" />
    <script>
        $('#datepicker').datepicker({
            uiLibrary: 'bootstrap4'
        });
    </script>
     
   </div>
    <a name="" id="" class="btn btn-secondary" href="db" role="button">Cancel</a>
    <button type="submit" class="btn btn-primary">Save</button>
    <a name="" id="" class="btn btn-danger"  href="complete_del/<%= results[0].id %>" role="button">Delete</a>
    </form>
  

    </div>


