<% include ../partials/header.ejs %>



<div class="container" style="margin-bottom:50px;">
<h2>ข้อมูลรายการสินค้า</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
            <th>Tag</th>
            <th>Date</th>
            <th><a name="" id="" class="btn btn-warning" href="insert_pd" role="button">Add New</a></th>
        </tr>
    </thead>
    <tbody>
    <% results.forEach(function(r) { %>
        <tr>
            <td><%= r.id %></td>
            <td><%= r.title %></td>
            <td><%= r.price %></td>
            <td><%= r.tag %></td>
            <td><%= r.create_at %></td>   
            <td><a name="" id="" class="btn btn-info" href="/product_list/<%= r.id %>" role="button">Edit</a></td>
            
        </tr>
    <% }); %>    
    </tbody>
</table>


</div>


<% include ../partials/footer.ejs %>